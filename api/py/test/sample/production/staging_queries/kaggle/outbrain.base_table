{
  "metaData": {
    "name": "kaggle.outbrain.base_table",
    "tableProperties": {
      "source": "chronon"
    },
    "outputNamespace": "default",
    "team": "kaggle"
  },
  "query": "\n        SELECT\n            clicks_train.display_id,\n            clicks_train.ad_id,\n            clicks_train.clicked,\n            events.document_id,\n            CAST(events.timestamp as LONG) + 1465876799998 as ts,\n            from_unixtime((CAST(events.timestamp as LONG) + 1465876799998)/1000, 'yyyy-MM-dd') as ds\n        FROM\n            kaggle_outbrain.clicks_train as clicks_train JOIN kaggle_outbrain.events as events ON clicks_train.display_id = events.display_id\n    "
}