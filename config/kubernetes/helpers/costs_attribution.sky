# DO NOT EDIT: http://go/vendor-skycfg
load("config/kubernetes/meta/metadata.sky", "labels")

def add_costs_attribution_override_label(ctx):
    """
    Add the cost attribution label to a pod if Henson passed a cost attribution variable.
    """
    costs_attribution_team = ctx.vars.get("service.config.costs_attribution_team")
    if costs_attribution_team == None:
        return None
    return labels({
        "stripe.io/costs-attribution-team": costs_attribution_team,
    })
